<view class="wrapper page">
<!-- 登录页 wrapper main -->
  <view class="main">
    <view class="log">
      <image src="../../images/mark-github.svg"></image>
    </view>
    
    <tabs>
      <tab title="账号密码">
      <form bindsubmit="commitAccount" report-submit='{{true}}'      class="from">
      <view>
        <input name="username" placeholder="输入账号" focus />
        <input name="password" placeholder="输入密码" password='true' value='{{password}}'/>

      </view>
      <button class="login" formType='submit'>登录</button>

      </form>
      </tab>
      <!-- Token为加密串 -->
      <tab title="Token"></tab>
    </tabs>
  </view>

</view>